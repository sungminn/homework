function setup() { 
  createCanvas(400, 400);
 
  background(0);
  colorMode(HSB);
	fill (255); 
  noStroke(); 


for (var y = 20; y <=380; y=y+20){
  var startX = y;
  var endX = 400-startX;
  if(y <= 200) {
  for (var x = y; x <= 400-y; x=x+20) {
    ellipse (x,y,18,18);
  }
} else {
  for (var x = endX; x<= startX; x=x+20) {
    ellipse(x,y,18,18) 
			}
		}
	}
}
